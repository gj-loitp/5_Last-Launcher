<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ColorSeekBar">
        <attr name="colorSeeds" format="reference" />
        <attr name="barHeight" format="dimension" />
        <attr name="thumbHeight" format="dimension" />
        <attr name="showAlphaBar" format="boolean" />
        <attr name="alphaBarPosition" format="integer" />
        <attr name="colorBarPosition" format="integer" />
        <attr name="bgColor" format="color" />
        <attr name="barRadius" format="dimension" />
        <attr name="barMargin" format="dimension" />
        <attr name="maxPosition" format="integer" />
        <attr name="isVertical" format="boolean" />
        <attr name="disabledColor" format="color" />
        <attr name="showThumb" format="boolean" />
    </declare-styleable>
    <declare-styleable name="FlowLayout">
        <attr name="android:orientation" />
        <attr name="layoutDirection" format="enum">
            <enum name="ltr" value="0" />
            <enum name="rtl" value="1" />
        </attr>
        <attr name="debugDraw" format="boolean" />
        <attr name="maxLines" format="integer" />
        <attr name="weightDefault" format="float" />
        <attr name="android:gravity" />
    </declare-styleable>
    <declare-styleable name="FlowLayout_LayoutParams">
        <attr name="layout_newLine" format="boolean" />
        <attr name="layout_weight" format="float" />
        <attr name="android:layout_gravity" />
    </declare-styleable>
</resources>
